input NodeInput {
    uuid: UUID!
}

input NodesInput {
    uuids: [UUID!]!
}

input PageInput {
    itemsPerPage: Int = 10
    pageNumber: Int = 0
}

interface ConnectionPayload {
    items: [Node!]!
    pageInfo: PageInfo!
    totalItems: Int!
}

interface Node {
    uuid: UUID!
}

interface Nodes {
    uuids: [UUID!]!
}

type DeletedNodes implements Nodes {
    uuids: [UUID!]!
}

type PageInfo {
    currentPage: Int!
    totalPages: Int!
}



scalar UUID
scalar ZonedDateTime
scalar Upload