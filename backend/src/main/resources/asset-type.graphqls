type AssetType implements Node {
    id: UUID!
    title: String
    icon: String
}

type AssetTypeConnection implements ConnectionPayload {
    items: [AssetType!]!
    pageInfo: PageInfo!
    totalItems: Int!
}

input AssetTypeInput {
    id: UUID!
}

type AssetTypePayload {
    item: AssetType
}

input CreateAssetTypeInput {
    title: String!
}
