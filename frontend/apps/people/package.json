{
    "name": "people",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "PORT=3001 craco start",
        "build": "craco build",
        "build:serve": "serve build -p 3001",
        "test": "craco test",
        "eject": "react-scripts eject",
        "lint": "eslint --ext .ts,.tsx ./src",
