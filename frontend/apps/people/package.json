{
    "name": "people",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "PORT=3001 env-cmd -f ./environments/.develop.env craco start",
        "dev:staging": "PORT=3001 env-cmd -f ./environments/.staging.env craco start",
        "build": "env-cmd -f ./environments/.develop.env craco build",
        "build:staging": "env-cmd -f ./environments/.staging.env craco build",
        "build:serve": "serve build -p 3001",
        "test": "craco test",
        "eject": "react-scripts eject",
        "lint": "eslint --ext .ts,.tsx ./src",
        "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
        "clear-all": "rm -rf build node_modules",
        "re-start": "rm -rf build node_modules && pnpm install && pnpm start",
        "re-build": "rm -rf build node_modules && pnpm install && pnpm build",
        "unused-exports": "./node_modules/.bin/ts-unused-exports tsconfig.json --searchNamespaces --ignoreFiles=.*generated --excludePathsFromReport=\"index\" --showLineNumber",
        "codegen": "graphql-codegen --overwrite --config codegen.develop.yml",
        "codegen:staging": "graphql-codegen --overwrite --config codegen.staging.yml"
    },
    "dependencies": {
        "@date-io/date-fns": "^2.14.0",
        "@mui/material": "^5.6.0",
        "flagged": "^2.0.3",
        "@apollo/client": "^3.6.2",
        "@apollo/link-ws": "2.0.0-beta.3",
        "apollo-link-context": "^1.0.20",
        "apollo-upload-client": "^17.0.0",
        "graphql": "^16.5.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-helmet-async": "^1.2.3",
        "react-router-dom": "^5.3.1",
        "react-scripts": "5.0.1",
        "stylis": "^4.1.1",
        "subscriptions-transport-ws": "^0.11.0",
        "wdx": "workspace:*",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@babel/core": "^7.17.9",
        "@babel/plugin-syntax-flow": "^7.16.7",
        "@babel/plugin-transform-react-jsx": "^7.14.9",
        "@craco/craco": "^6.4.3",
        "@graphql-codegen/cli": "^2.6.2",
        "@graphql-codegen/near-operation-file-preset": "^2.2.12",
        "@graphql-codegen/typescript": "^2.4.11",
        "@graphql-codegen/typescript-operations": "^2.4.0",
        "@graphql-codegen/typescript-react-apollo": "^3.2.14",
        "@graphql-codegen/typescript-react-query": "^3.5.12",
        "@testing-library/dom": "8.13.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.1.1",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.4.1",
        "@types/node": "^16.11.27",
        "@types/react": "^18.0.6",
        "@types/react-dom": "^18.0.2",
        "@types/react-router-dom": "^5.3.3",
        "config": "workspace:*",
        "env-cmd": "^10.1.0",
        "graphql-tag": "^2.12.6",
        "prettier": "^2.6.2",
        "serve": "^13.0.2",
        "ts-unused-exports": "^8.0.0",
        "tsconfig": "workspace:*",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "tslib": "^2.4.0",
        "typescript": "^4.6.3"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not op_mini all"
    ]
}
