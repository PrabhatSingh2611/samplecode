{
    "name": "host",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "craco start -p 3000",
        "build": "craco build",
        "build:serve": "serve build -p 3000",
        "test": "craco test",
        "eject": "react-scripts eject",
        "lint": "eslint --ext .ts,.tsx ./src",
        "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
        "clear-all": "rm -rf build node_modules",
        "re-start": "rm -rf build node_modules && pnpm install && pnpm start",
        "re-build": "rm -rf build node_modules && pnpm install && pnpm build",
        "unused-exports": "ts-unused-exports tsconfig.json --searchNamespaces --ignoreFiles=.*generated --excludePathsFromReport=\"index\" --showLineNumber",
        "codegen:local": "SCHEMA_PATH=http://localhost:8080 graphql-codegen --overwrite --config codegen.yml",
        "codegen:develop": "SCHEMA_PATH=https://audra-service.dev.topazdigital.cloud/graphql graphql-codegen --overwrite --config codegen.yml",
        "analyze": "pnpm dlx source-map-explorer 'build/**/*.js' --gzip",
        "analyze:no-gzip": "pnpm dlx source-map-explorer 'build/**/*.js'"
    },
    "dependencies": {
        "flagged": "^2.0.3",
        "@apollo/client": "^3.6.2",
        "@apollo/link-ws": "2.0.0-beta.3",
        "@emotion/cache": "^11.7.1",
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@iconify/react": "^3.2.1",
        "@mui/material": "^5.6.0",
        "change-case": "^4.1.2",
        "date-fns": "^2.28.0",
        "framer-motion": "^6.2.9",
        "graphql": "^16.5.0",
        "jsonschema": "^1.4.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-error-boundary": "^3.1.4",
        "react-helmet-async": "^1.2.3",
        "react-lazy-load-image-component": "^1.5.4",
        "react-router-dom": "^5.3.1",
        "react-scripts": "5.0.1",
        "serve": "^13.0.2",
        "simplebar": "^5.3.6",
        "simplebar-react": "^2.3.6",
        "stylis": "^4.1.1",
        "stylis-plugin-rtl": "^2.1.1",
        "subscriptions-transport-ws": "^0.11.0",
        "url": "^0.11.0",
        "wdx": "workspace:*",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@babel/core": "^7.17.9",
        "@babel/plugin-syntax-flow": "^7.16.7",
        "@babel/plugin-transform-react-jsx": "^7.14.9",
        "@craco/craco": "^6.4.3",
        "@graphql-codegen/cli": "^2.6.2",
        "@graphql-codegen/near-operation-file-preset": "^2.2.12",
        "@graphql-codegen/typescript": "^2.4.11",
        "@graphql-codegen/typescript-operations": "^2.4.0",
        "@graphql-codegen/typescript-react-apollo": "^3.2.14",
        "@graphql-codegen/typescript-react-query": "^3.5.12",
        "@testing-library/dom": "8.13.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.1.1",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.4.1",
        "@types/node": "^16.11.27",
        "@types/react": "^18.0.6",
        "@types/react-dom": "^18.0.2",
        "@types/react-lazy-load-image-component": "^1.5.2",
        "@types/react-router-dom": "^5.3.3",
        "config": "workspace:*",
        "graphql-tag": "^2.12.6",
        "prettier": "^2.6.2",
        "tsconfig": "workspace:*",
        "tslib": "^2.4.0",
        "ts-unused-exports": "^8.0.0",
        "typescript": "^4.6.3"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not op_mini all"
    ]
}